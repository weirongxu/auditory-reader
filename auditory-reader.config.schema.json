{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "SpeechConfig",
  "type": "object",
  "properties": {
    "appPort": {
      "type": "integer"
    },
    "appPublicPath": {
      "type": "string"
    },
    "appBodyLimit": {
      "type": "string"
    },
    "accounts": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "account": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        }
      }
    },
    "sessionKey": {
      "type": "string"
    }
  },
  "required": ["accounts", "sessionKey"]
}
