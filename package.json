{
  "name": "auditory-reader",
  "version": "1.0.0",
  "private": true,
  "description": "A Speech Reader, Support Epub, Text",
  "type": "module",
  "scripts": {
    "build:sw": "webpack build --mode production --env=app_public_path=/ --env app_mode=service-worker",
    "build:server": "run-s build:server:node build:server:web",
    "build:server:node": "tsc -p tsconfig.build.json",
    "build:server:web": "webpack build --mode production --env app_mode=service-worker --env app_mode=server",
    "dev:sw": "webpack-dev-server --port 4002 --mode development --env=app_public_path=/ --env app_mode=service-worker",
    "dev:server": "run-p dev:server:node dev:server:web",
    "dev:server:web": "webpack-dev-server --port 4000 --mode development --env app_mode=server",
    "dev:server:node": "ts-node-esm src/server/index.ts",
    "predeploy": "run-s build:sw",
    "deploy": "gh-pages -d sw-public",
    "start": "node lib/server/index.js",
    "lint": "eslint src --ext tsx,ts"
  },
  "dependencies": {
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.2",
    "@react-hookz/web": "^20.0.3",
    "@types/jsdom": "^20.0.1",
    "@types/mime-types": "^2.1.1",
    "@xmldom/xmldom": "^0.8.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "howler": "^2.2.3",
    "i18next": "^22.4.7",
    "is-plain-obj": "^4.1.0",
    "jsdom": "^20.0.3",
    "jszip": "^3.10.1",
    "material-ui-confirm": "^3.0.7",
    "mime-types": "^2.1.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hooks-global-state": "^2.1.0",
    "react-i18next": "^12.1.1",
    "react-router-dom": "^6.6.1",
    "tsconfig-paths": "^4.1.2",
    "type-fest": "^3.5.0",
    "use-window-focus": "^1.4.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@emotion/react": "^11.10.5",
    "@raidou/eslint-config-react": "^1.2.0",
    "@types/babel__core": "^7.1.20",
    "@types/dom-screen-wake-lock": "^1.0.0",
    "@types/eslint": "^8.4.10",
    "@types/express": "^4.17.15",
    "@types/express-session": "^1.17.5",
    "@types/file-loader": "^5.0.1",
    "@types/howler": "^2.2.7",
    "@types/node": "18.11.18",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/session-file-store": "^1.2.2",
    "@types/uuid": "^8.3.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "eslint": "^8.31.0",
    "eslint-plugin-import": "^2.26.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "idb": "^7.1.1",
    "mini-css-extract-plugin": "^2.7.2",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "session-file-store": "^1.5.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.9.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "engines": {
    "node": ">=14"
  }
}
