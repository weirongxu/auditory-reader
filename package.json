{
  "name": "auditory-reader",
  "version": "1.0.0",
  "private": true,
  "description": "A Speech Reader, Support Epub, Text",
  "type": "module",
  "scripts": {
    "build:sw": "webpack build --mode production --env=app_public_path=/ --env app_mode=service-worker",
    "build:server": "run-s build:server:node build:server:web",
    "build:server:node": "tsc -p tsconfig.build.json",
    "build:server:web": "webpack build --mode production --env app_mode=service-worker --env app_mode=server",
    "dev:sw": "webpack-dev-server --port 4002 --mode development --env=app_public_path=/ --env app_mode=service-worker",
    "dev:server": "run-p dev:server:node dev:server:web",
    "dev:server:web": "webpack-dev-server --port 4000 --mode development --env app_mode=server",
    "dev:server:node": "ts-node-esm src/server/index.ts",
    "test": "jest src",
    "test:watch": "jest src --watch",
    "predeploy": "webpack build --mode production --env=app_public_path=/auditory-reader/ --env app_mode=service-worker",
    "deploy": "gh-pages -d sw-public",
    "start": "node lib/server/index.js",
    "lint": "eslint src --ext tsx,ts"
  },
  "dependencies": {
    "@emotion/styled": "^11.11.0",
