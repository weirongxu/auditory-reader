    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "howler": "^2.2.4",
    "htmlparser2": "^9.1.0",
    "i18next": "^23.11.1",
    "invert-color": "^2.0.0",
    "is-plain-obj": "^4.1.0",
